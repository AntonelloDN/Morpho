{
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "path": "Morpho.zip",
                        "name": "Morpho-${nextRelease.version}.zip",
                        "label": "Morpho-${nextRelease.version}.zip"
                    }

                ]
            }
        ]
    ]
}

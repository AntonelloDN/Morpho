{
    "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        [
            "@semantic-release/github",
            {
                "assets": [
                    {
                        "path": "installer/Morpho.Rhino.Installer.exe",
                        "name": "Morpho.Rhino.Installer-${nextRelease.version}.exe",
                        "label": "Morpho.Rhino.Installer-${nextRelease.version}.exe"
                    },
                    {
                        "path": "Examples.zip",
                        "name": "Examples-${nextRelease.version}.zip",
                        "label": "Examples-${nextRelease.version}.zip"
                    }
                ]
            }
        ]
    ]
}
